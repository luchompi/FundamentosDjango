      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}\\manage.py",
      "args": ["runserver"],
      "django": true
    },
    {
      "name": "Construir tablas 🏗️",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}\\manage.py",
      "args": ["makemigrations"],
      "django": true
    },
    {
        "name": "Migrar 💾",
        "type": "debugpy",
        "request": "launch",
        "program": "${workspaceFolder}\\manage.py",
        "args": ["migrate"],
        "django": true
      },

  ]
}
